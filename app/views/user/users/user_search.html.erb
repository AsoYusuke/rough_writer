<div class="container">
  <h2>検索結果</h2>
  (全<%= @search_users.count %>件)
   <div class="row">
    <div class="col-1"></div>
     <div class="col-md-10 mx-auto">
       <div class="list-wrap">
        <% @search_users.each do |user| %>
       <div class="list-box">
       <div class="image_wrap">
        <%= link_to user_path(user.id) do %>
        <%= attachment_image_tag user, :profile_image, size: '200x150', class:"attachment product product_image fallback" %>
        <% end %></div>
       <div class="list-comment">
        <span class="list-name"><%= user.name %></span><br />
        <!--<span class="list-price">￥<#%= user.sales_price.to_s(:delimited) %></span>-->
       </div>
       </div>
       <% end %>
     </div>
    
    </div>
   <div class="col-md-1"></div>
  </div>
</div>
</html>
