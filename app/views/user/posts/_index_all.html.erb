<% posts.each do |post| %>
  <%= link_to post_path(post) do %>
      <%= attachment_image_tag post, :image, size: "190x190", class: "coffee_image_media" %>
  <% end %>
  <%= link_to user_path(post.user_id) do %>
      <%= attachment_image_tag post.user, :profile_image, fallback: "no_image.jpg", class:"profile-img-circle", size: "30x30" %>
      <%= post.user.name %>
  <% end %>
  <%= post.title %>
  <div id="index_good_<%= post.id %>">
    <%= render 'user/goods/good', post: post %>
  </div>
  <div id="index_bad_<%= post.id %>">
    <%= render 'user/bads/bad', post: post %>
  </div>
<% end %>